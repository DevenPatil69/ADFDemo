{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFdemodeven"
		},
		"linkedservices_demodeven_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservices_demodeven'"
		},
		"sample_linked_service_for_github_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sample_linked_service_for_github'"
		},
		"Linked_service_sql_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().servername}"
		},
		"Linked_service_sql_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbname}"
		},
		"Linked_service_sql_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().username}"
		},
		"trigger1_properties_pipeline2_parameters_DBnameFromPipeline": {
			"type": "string",
			"defaultValue": "Table1"
		},
		"trigger1_properties_pipeline2_parameters_SourceTable": {
			"type": "string",
			"defaultValue": "TestDB"
		},
		"trigger1_properties_pipeline2_parameters_DestinationTable": {
			"type": "string",
			"defaultValue": "Table2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFdemodeven_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedservices_demodeven_connectionString": {
						"value": "[parameters('linkedservices_demodeven_connectionString')]"
					},
					"sample_linked_service_for_github_connectionString": {
						"value": "[parameters('sample_linked_service_for_github_connectionString')]"
					},
					"Linked_service_sql_server_properties_typeProperties_server": {
						"value": "[parameters('Linked_service_sql_server_properties_typeProperties_server')]"
					},
					"Linked_service_sql_server_properties_typeProperties_database": {
						"value": "[parameters('Linked_service_sql_server_properties_typeProperties_database')]"
					},
					"Linked_service_sql_server_properties_typeProperties_userName": {
						"value": "[parameters('Linked_service_sql_server_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFdemodeven_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger1_properties_pipeline2_parameters_DBnameFromPipeline": {
						"value": "[parameters('trigger1_properties_pipeline2_parameters_DBnameFromPipeline')]"
					},
					"trigger1_properties_pipeline2_parameters_SourceTable": {
						"value": "[parameters('trigger1_properties_pipeline2_parameters_SourceTable')]"
					},
					"trigger1_properties_pipeline2_parameters_DestinationTable": {
						"value": "[parameters('trigger1_properties_pipeline2_parameters_DestinationTable')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFdemodeven_ArmTemplate_0')]"
			]
		}
	]
}